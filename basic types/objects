
let employee: object; 
employee = {
    firstName: 'John',
    lastName: 'Doe',
    age: 25,
    jobTitle: 'Web Developer'
};

console.log(employee);

output
[LOG]: {
  "firstName": "John",
  "lastName": "Doe",
  "age": 25,
  "jobTitle": "Web Developer"
} 
*****************************************************************************
let employee: {
    firstName: string;
    lastName: string;
    age: number;
    jobTitle: string;
} = {
    firstName: 'John',
    lastName: 'Doe',
    age: 25,
    jobTitle: 'Web Developer'
};

output

[LOG]: {
  "firstName": "John",
  "lastName": "Doe",
  "age": 25,
  "jobTitle": "Web Developer"
} 
