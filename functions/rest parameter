syntax

function fn(...variable_name:number[]):number
{ 
  statements...
}

example 1

  function getTotal(...numberes:number[]):number
  {
    let total=0;
    numberes.foreach((num)=> total +=num);
    return total
  }


console.log(getTotal()); // 0
console.log(getTotal(10, 20)); // 30
console.log(getTotal(10, 20, 30)); // 60
