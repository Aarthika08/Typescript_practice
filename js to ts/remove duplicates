for (let i:number=0;i<n;i++){
subject[i]=Number(prompt("Please enter number:"));
}
function removeDuplicates(arr:number) {
    return subject.filter((item,
        index) => subject.indexOf(item) === index);
}
    
console.log(removeDuplicates(subject));
else{
    let result:string="fail"
    console.log("Hello, your grade is D  and your result is " + result + "! and your total is "+total);

