let name:string=prompt('enter dept name');
let dict:{[key:string]:string[]}={"Engineerring":["EEE","ECE","CSE"],"Arts":["BSC","BA"]};


console.log("student,list",search(name));

function search(name1:string):string[]|undefined
{
    for(let key  in dict)
    {
        if(name1=== key)
        {
            return dict[key];
        }
    }
}


